{
  "name": "misfist/missdrepants-network",
  "version": "1.0.0",
  "homepage": "https://github.com/misfist/missdrepants-network",
  "description": "",
  "keywords": [
    "php"
  ],
  "authors": [
    {
      "name": "Pea",
      "homepage": "https://github.com/misfist",
      "role": "Developer"
    }
  ],
  "license": "GPLv3",
  "type": "wordpress-project",
  "repositories": [
    {
      "type":"composer",
      "url":"http://wpackagist.org"
    },
    {
      "type":"package",
      "package": {
        "name": "misfist/loom",
        "type": "wordpress-theme",
        "version":"dev-master",
        "source": {
          "url": "https://github.com/misfist/loom-theme.git",
          "type": "git",
          "reference":"master"
        }
      }
    },
    {
      "type":"package",
      "package": {
        "name": "misfist/missdrepants-child",
        "version":"dev-master",
        "type": "wordpress-theme",
        "source": {
          "url": "https://github.com/misfist/missdrepants-child.git",
          "type": "git",
          "reference":"master"
        }
      }
    },
    {
      "type":"package",
      "package": {
        "name": "misfist/thegoldenbelle-child",
        "version":"dev-master",
        "type": "wordpress-theme",
        "source": {
          "url": "https://github.com/misfist/thegoldenbelle-child.git",
          "type": "git",
          "reference":"master"
        }
      }
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "wpackagist-plugin/akismet": ">=3.1.7",
    "wpackagist-plugin/codepress-admin-columns": ">=2.4.8",
    "wpackagist-plugin/intuitive-custom-post-order": ">=3.0.7",
    "wpackagist-plugin/check-email": ">=0.3",
    "wpackagist-plugin/contact-form-7": ">=4.3.1",
    "wpackagist-plugin/google-analytics-dashboard-for-wp": ">=4.9.2",
    "wpackagist-plugin/simple-file-downloader": ">=1.0.4",
    "wpackagist-plugin/wordpress-mu-domain-mapping": ">=0.5.5.1",
    "wpackagist-plugin/wp-user-avatar": ">=1.9.13",
    "wpackagist-plugin/easy-testimonials": ">=1.34.3",
    "wpackagist-plugin/category-posts": ">=4.1.4",
    "wpackagist-plugin/simply-instagram": "1.3.3",
    "wpackagist-plugin/wp-instagram-widget": ">=1.9.4",
    "wpackagist-plugin/yikes-inc-easy-mailchimp-extender": ">=6.0.3.8",
    "wpackagist-plugin/jetpack": ">=3.9.1",
    "wpackagist-plugin/jetpack-module-control": ">=1.3",
    "wpackagist-plugin/updraftplus": ">=1.11.24",
    "wpackagist-plugin/yelp-widget-pro": ">=1.4.2",
    "wpackagist-plugin/siteorigin-panels": ">=2.2.2",
    "wpackagist-plugin/wordpress-seo": ">=3.0.6",
    "wpackagist-plugin/wp-super-cache": ">=1.4.6",
    "wpackagist-plugin/wordpress-importer": ">=0.6.1",
    "misfist/loom": "dev-master",
    "misfist/missdrepants-child": "dev-master",
    "misfist/thegoldenbelle-child": "dev-master"
  },
  "require-dev": {
    "wpackagist-plugin/velvet-blues-update-urls": "*",
    "wpackagist-plugin/developer": "*",
    "wpackagist-plugin/which-template-file": "*",
    "wpackagist-plugin/wp-log-viewer": "*"
  },
  "extra"       : {
    "installer-paths": {
        "./plugins/{$name}/": ["type:wordpress-plugin"],
        "./themes/{$name}/": ["type:wordpress-theme"]
    }
  }
}
